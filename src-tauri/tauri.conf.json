{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "discordexportdatareader",
  "version": "0.1.0",
  "identifier": "work.inagi-saki.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "discordexportdatareader",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": "default-src 'self' img-src 'self' asset: http://asset.localhost",
      "assetProtocol": {
        "enable": true,
        "scope": ["$ASSET/ExportData/assets","$ASSET/ExportData/assets/*"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
